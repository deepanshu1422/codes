#include<iostream>
#include<vector>
using namespace std;

int main(){
    vector<int> v;
    v.push_back(10);
    v.push_back(20);
    v.push_back(30);

    cout<<v[0]<<endl;
    cout<<v[1]<<endl;
    cout<<v[2]<<endl;

    v[1]=100;
    cout<<v[1]<<endl;
    cout<<v.size()<<endl;

    for(int i = 0;i<10; i++){
            cout<<v.capacity()<<endl;
            v.push_back(i+1);
            cout<<v.size()<<endl;
    }
}
