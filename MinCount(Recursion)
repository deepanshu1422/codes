#include<iostream>
using namespace std;

int minCount(int n){
/*    if(n==0)
        return 0;
    if(n==1)
        return 1;
    if(n==2)
        return 2;
    if(n==3)
        return 3;
    int ans=INT_MAX;
    int i=1;
    while((i*i)<=n){
        int x = minCount(n-(i*i));
        ans = min(x+1,ans);
        i++;
    }
    return ans;
}

int main(){
    
    int num;
    cin >> num;
    cout << minCount(num);
    
}

